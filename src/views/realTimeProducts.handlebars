<div>
  <title>{{title}}</title>
  <table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Nombre</th>
        <th>Description</th>
        <th>Code</th>
        <th>Precio</th>
        <th>Stock</th>
        <th>Category</th>
        <th>Acci√≥n</th>
      </tr>
    </thead>
    <tbody id="productRows">
      {{#each product}}
      <tr id="product-{{this.id}}">
        <td>{{this.id}}</td>
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>{{this.code}}</td>
        <td>{{this.price}}</td>
        <td>{{this.stock}}</td>
        <td>{{this.category}}</td>
        <td>
          <button class="btn-delete" data-id="{{this.id}}">Borrar</button>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>