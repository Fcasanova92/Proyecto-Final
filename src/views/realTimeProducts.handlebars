
<div>
  <h1>Lista de Productos</h1>
  <ul id="product-list">
    {{#each products}}
      <li>
        {{this.name}} - ${{this.price}}
        <button data-id="{{this.id}}" class="delete-btn">Eliminar</button>
      </li>
    {{/each}}
  </ul>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>
</div>

